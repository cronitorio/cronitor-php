{
  "name": "cronitor/cronitor-php",
  "description": "Cronitor PHP Library",
  "keywords": [
    "cronitor",
    "api"
  ],
  "homepage": "https://cronitor.io/",
  "license": "MIT",
  "authors": [
    {
      "name": "Cronitor"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/yaml": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "friendsofphp/php-cs-fixer": "^2.18",
    "ext-curl": "*",
    "codeception/aspect-mock": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Cronitor\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cronitor\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit ./tests"
  }
}
